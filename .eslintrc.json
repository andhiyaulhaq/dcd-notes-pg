{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "no-console": 0,
			"linebreak-style": 0,
			"no-unused-vars": 0
    }
}
